{
  "name": "@coderspirit/nominal",
  "version": "3.2.2",
  "description": "Powerful nominal types for your project",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "node": "./dist/cjs/index.js"
  },
  "keywords": [
    "typescript",
    "types",
    "nominal",
    "taint"
  ],
  "author": "Andres Correa Casablanca <castarco@coderspirit.xyz>",
  "private": false,
  "license": "MIT",
  "scripts": {
    "build": "yarn build:npm && yarn build:deno",
    "build:cjs": "tsc -p ./tsconfig.json",
    "build:deno": "./scripts/build_deno.sh",
    "build:esm": "./scripts/build_esm.sh",
    "build:npm": "yarn build:cjs && yarn build:esm",
    "clean": "yarn clean:dist && yarn clean:deno",
    "clean:deno": "rm -rf ./deno/*.ts && rm -rf ./deno/*/*.ts",
    "clean:dist": "rm -rf ./dist",
    "lint": "yarn lint:tsc && yarn lint:eslint",
    "lint:eslint": "eslint . --ext .js,.ts --max-warnings 0",
    "lint:tsc": "yarn lint:tsc:main && yarn lint:tsc:tests",
    "lint:tsc:main": "tsc -p ./tsconfig.json --noEmit",
    "lint:tsc:tests": "tsc -p ./src/__tests__/tsconfig.json --noEmit",
    "prepublishOnly": "yarn lint && yarn test && yarn clean:dist && yarn build:npm",
    "test": "jest"
  },
  "dependencies": {
    "@coderspirit/nominal-symbols": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.2.1",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "settings": {
    "jest": {
      "version": 27
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Coder-Spirit/nominal.git"
  },
  "bugs": {
    "url": "https://github.com/Coder-Spirit/nominal/issues"
  },
  "homepage": "https://github.com/Coder-Spirit/nominal#readme"
}
